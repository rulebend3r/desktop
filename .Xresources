*foreground: #bf8cff
*foreground_bold: #a8a19f
*cursor: #00cc00
*background: #000000
!!*background: rgba(0, 0, 0, .7)


URxvt.urgentOnBell: off
URxvt.visualBell: false
URxvt.iso14755: false
URxvt.iso14755_52: off
URxvt.meta8: true
URxvt.depth: 32
URxvt.iconFile: /usr/share/icons/NewAzenisIcons/scalable/apps/xfce-terminal.png
URxvt.metaSendsEscape: true

URxvt.internalBorder: 0
URxvt.externalBorder: 0

URxvt.scrollBar: off
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.cursorColor:#00AA00
URxvt.saveLines: 10000
URxvt.scrollTtyOutput:   false
URxvt.scrollWithBuffer:  true
URxvt.scrollTtyKeypress: true

urxvt*depth:32
urxvt*foreground:#BF8CFF
urxvt*background:[70]#000000
URxvt.foreground: 15
URxvt.geometry: 91x21

! Ctrl-Tab, Ctrl-Shift-Tab and other bindings in urxvt
URxvt.keysym.Control-Up:   \033[1;5A
URxvt.keysym.Control-Down:    \033[1;5B
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C

!!ProFont
URxvt.font: xft:Profont for Powerline:pixelsize=12:antialias=true:hinting=true
URxvt.boldFont: xft:ProFont Bold for Powerline:bold:pixelsize=12:antialias=true:hinting=true
URxvt.italicFont: xft:ProFont for Powerline:italic:pixelsize=12:antialias=true:hinting=true
URxvt.boldItalicFont: xft:ProFont for Powerline Bold for Powerline:bold:italic:pixelsize=12:antialias=true
!!URxvt.letterSpace: 0

!URxvt.font:           xft:Terminus (TTF) for Powerline:MediumForPowerline:size=13:antialias=true,xft:Inconsolata\ For\ Powerline:size=13:antialias=true
!URxvt.imFont:         xft:Terminus (TTF) for Powerline:MediumForPowerline:size=13:antialias=true,xft:Inconsolata\ For\ Powerline:size=13:antialias=true
!URxvt.boldFont:       xft:Terminus (TTF) for Powerline:MediumForPowerline:size=13:antialias=true,xft:Inconsolata\ For\ Powerline:size=13:antialias=true
!URxvt.italicFont:     xft:Terminus (TTF) for Powerline:MediumForPowerline:size=13:antialias=true,xft:Inconsolata\ For\ Powerline:size=13:antialias=true
!URxvt.boldItalicFont: xft:Terminus (TTF) for Powerline:MediumForPowerline:size=13:antialias=true,xft:Inconsolata\ For\ Powerline:size=13:antialias=true

! Disable screen printing by overriding the default print pipe.
URxvt.print-pipe: "cat >/dev/null"

URxvt.perl-ext-common : default,keyboard-select,url-select,clipboard,ibeamcursor,matcher

URxvt.keysym.M-x           	: perl:keyboard-select:activate
URxvt.keysym.M-u           	: perl:url-select:select_next
URxvt.keysym.M-c           	: perl:clipboard:copy
URxvt.keysym.M-v           	: perl:clipboard:paste
URxvt.keysym.M-s           	: perl:keyboard-select:search
URxvt.keysym.M-C-v         	: perl:clipboard:paste_escaped
!Urxvt.perl-lib            	: /usr/lib/urxvt/perl/
URxvt.url-select.launcher  	: luakit
URxvt.url-select.autocopy  	: true
URxvt.url-select.underline 	: true

URxvt.url-select.button:   1

!URxvt.clipboard.copycmd:  xsel --input  --clipboard
!URxvt.clipboard.pastecmd: xsel --output --clipboard

!URxvt.keysym.Control-Shift-C: perl:clipboard:copy
!URxvt.keysym.Control-Shift-V: perl:clipboard:paste

! Black
URxvt.color0:  #000000
URxvt.color8:  #000000
! Red
URxvt.color1:  #cd0000
URxvt.color9:  #ff0000
! Green
URxvt.color2:  #00cd00
URxvt.color10: #00ff00
! Yellow
URxvt.color3:  #cdcd00
URxvt.color11: #ffff00
! Blue
URxvt.color4:  #1e90ff
URxvt.color12: #4682b4
! Magenta
URxvt.color5:  #cd00cd
URxvt.color13: #ff00ff
! Cyan
URxvt.color6:  #00cdcd
URxvt.color14: #00ffff
! White
URxvt.color7:  #FFFFFF
URxvt.color15: #FFFFFF

